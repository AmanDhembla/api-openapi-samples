  /{idA2A}/PA/{codEnte}/disponibilita/ack:
    get:
      description: |
        TESORIERE: Lista Ack Disponibilità liquide relative ad uno specifico Ente
      parameters:
      - $ref: '#/components/parameters/idA2A'
      - $ref: '#/components/parameters/codEnte'
