      tags:
      - disponibilitaLiquide


components:
  parameters:
    idA2A:
      in: path
      name: idA2A
      required: true
      schema:
        type: string
      description: identificativo A2A del tramite della PA che origina la richiesta
    codEnte:
      in: path
      name: codEnte
      required: true
      schema:
        type: string
      description: codice IPA della PA
    codBanca:
      in: path
      name: codBanca
      required: true
      schema:
        type: string
      description: codice ABI della BT
    progFlusso:
      in: path
      name: progFlusso
      required: true
      schema:
        type: string
      description: >-
            Progressivo assegnato da SIOPE+ all'atto dell'invio del flusso
            ordinativi
    progEsitoApplicativo:
      in: path
      name: progEsitoApplicativo
      required: true
      schema:
        type: string
      description: >-
            Progressivo assegnato da SIOPE+ all'atto dell'invio dell'esito
            applicativo
    progGiornale:
      in: path
      name: progGiornale
      required: true
      schema:
        type: string
      description: >-
            Progressivo assegnato da SIOPE+ all'atto dell'invio del giornale
            di cassa
    pagina:
      in: query
      name: pagina
      schema:
        type: number
        format: integer
        default: 1
      description: numero della pagina dei risultati richiesta
    download:
      in: query
      name: download
      schema:
        type: boolean
      description: Filtro dei risultati sul fatto che siano stati già scaricati o meno.
    dataUploadDa:
      in: query
      name: dataUploadDa
      schema:
        type: string
        format: date-time
      description: >-
            Filtro dei risultati sulla data di upload iniziale nel formato
            `yyyy-MM-dd'T'HH:mm:ss.SSS`
    dataUploadA:
      in: query
      name: dataUploadA
      schema:
        type: string
        format: date-time
      description: >-
            Filtro dei risultati sulla data di upload finale nel formato
            `yyyy-MM-dd'T'HH:mm:ss.SSS`
    dataProduzioneDa:
      in: query
      name: dataProduzioneDa
      schema:
        type: string
        format: date-time
      description: >-
            Filtro dei risultati sulla data di produzione iniziale nel formato
            `yyyy-MM-dd'T'HH:mm:ss.SSS`
    dataProduzioneA:
      in: query
      name: dataProduzioneA
      schema:
        type: string
        format: date-time
      description: >-
            Filtro dei risultati sulla data di produzione finale nel formato
            `yyyy-MM-dd'T'HH:mm:ss.SSS`
    progDisponibilita:
      description: Progressivo assegnato da SIOPE+ all'atto dell'invio delle disponibilità liquide
      in: path
      name: progDisponibilita
      required: true
      schema:
        type: string
  schemas:
    FlussoDataUpload:
      type: object
      properties:
        progFlusso:
          type: string
          description: progressivo assegnato al flusso da SIOPE+
          example: '1234567890'
        dataUpload:
          type: string
          format: date-time
          description: >-
            data di invio del flusso, nel formato
            `yyyy-MM-dd'T'HH:mm:ss.SSS`
          example: '2016-12-12T15:44:59.789'
        download:
          type: boolean
          description: >-
            flag che indica se il flusso sia già stato scaricato o
            meno
          example: 'false'
        location:
          type: string
          format: uri
          description: URL dove reperire il flusso inviato

    FlussoDataProduzione:
      type: object
      properties:
        progFlusso:
          type: string
          description: progressivo assegnato al flusso da SIOPE+
          example: '1234567890'
        dataProduzione:
          type: string
          format: date-time
          description: >-
            data di produzione del flusso, nel formato
            `yyyy-MM-dd'T'HH:mm:ss.SSS`
          example: '2016-12-12T15:44:59.789'
        download:
          type: boolean
          description: >-
            flag che indica se il flusso sia già stato scaricato o
            meno
          example: 'false'
        location:
          type: string
          format: uri
          description: URL dove reperire il flusso inviato

    FlussiPaginatiUpload:
      type: object
      properties:
        numRisultati:
          type: number
          format: integer
          example: '12345'
        numPagine:
          type: number
          format: integer
          example: '123'
        risultatiPerPagina:
          type: number
          format: integer
          example: '100'
        pagina:
          type: number
          format: integer
          example: '1'
        risultati:
          type: array
          items:
            '$ref': '#/components/schemas/FlussoDataUpload'

    FlussiPaginatiProduzione:
      type: object
      properties:
        numRisultati:
          type: number
          format: integer
          example: '12345'
        numPagine:
          type: number
          format: integer
          example: '123'
        risultatiPerPagina:
          type: number
          format: integer
          example: '100'
        pagina:
          type: number
          format: integer
          example: '1'
        risultati:
          type: array
          items:
            '$ref': '#/components/schemas/FlussoDataProduzione'
